@ADVANCE_DEMONIEZD_HARD, ARG

IF CSTR:ARG:타이틀 == "악마인(극한광화)"
	;3回目
	TALENT:ARG:붕괴 = 1
	TALENT:ARG:연모 = 0
	TALENT:ARG:음란 = 0
	TALENT:ARG:복종 = 0
	TALENT:ARG:친애 = 0
	TALENT:ARG:창부 = 0
	TALENT:ARG:예속 = 0
	CSTR:ARG:타이틀 = 악마인(붕괴)
	RETURN 0
ELSEIF CSTR:ARG:타이틀 == "악마인(광화)" 
	;2回目
	TALENT:ARG:이형 = 1
	TALENT:ARG:짐승 = 0
	TALENT:ARG:새 = 0
	TALENT:ARG:파충류 = 0
	TALENT:ARG:물고기 = 0
	TALENT:ARG:인간화 = 0
	CSTR:ARG:타이틀 = 악마인(극한광화)
	RETURN 1
ELSE
	;1回目
	TALENT:ARG:광기 = 1
	CSTR:ARG:타이틀 = 악마인(광화)
	RETURN 1
ENDIF

