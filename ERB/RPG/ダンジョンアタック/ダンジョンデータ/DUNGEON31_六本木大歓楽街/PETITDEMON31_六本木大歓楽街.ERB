;롯폰기대환락가
;プチデモ関連

;色々と확장の余地がありそうなので, 창관と同じく分割
;こちらも共通化できそうな部分を一本化

;デモナイズドについて色々捏造
;내부종족を악마인に統一し, デモナイズドした인간≒악마인という扱いに
;複数のデビル소스突っ込んで내성を강화する기능を추가

;フラグ管理メモ
;8　プチデモSHOP이용경험フラグ　0:未이용　1:이용したことある
;9　プチじゃないデモ이용경험フラグ　0:未이용　1:이용したことある　2:강화이용경험あり　


;=======================================================================
;プチデモ
;=======================================================================
@PETIT_DEMON_31
#DIM CHARA, 1
#DIMS VALUES
#DIM BF_FLAG, 1
#DIM サーチ変数, 1

CALL MESSAGE_WINDOW_D, "남자 점주", "쁘띠 데몬숍에 어서오세요♪"
IF 던전플래그:31:8 == 0
	던전플래그:31:8 = 1
	CALL MESSAGE_WINDOW_D, "", "＞염소 같은 뿔을 기른 울끈불끈한 남자가 인사해 왔…/＞악마가 아닌건가…?"
	CALL MESSAGE_WINDOW_D, "남자 점주", @"어라, 손님, 처음이에요?/괜찮아, 당신은 지금부터 유행의 최첨단을 달리게 될거에요/내가 제대로 설명해 줄·게·용%ハート()%"
	CALL MESSAGE_WINDOW_D, "", "＞근육질의 몸과 말투의 편차가 심한 점주는, 그렇게 말하곤 설명하기 시작했다…"
	CALL MESSAGE_WINDOW_D, "남자 점주", "여기는요, 악마의 모습을 자신의 몸에 넣을 수 있는 가게야/우리가 듬뿍 가진 악마 소스를 써용"
	;ＳＪのパロ디무です, ＩＭＡＧＩＮＥにはそんな情報有りません
	CALL MESSAGE_WINDOW_D, "남자 점주", "구체적으론, 동물의 귀를 달거나, 엉덩이에 꼬리를 달거나/날개를 달거나, 인어가 되는것 등을 할 수 있어"
	CALL MESSAGE_WINDOW_D, "남자 점주", "그리고, 당신들이 바란다면/쁘띠가 아닌 데모나이즈도 할 수 있어요/이건 별로 권하지 않지만…"
	CALL MESSAGE_WINDOW_D, "남자 점주", "자, 그러면…"
ENDIF
$P_31

VALUES = 오늘은 무슨 일일까?//
VALUES += "[1]쁘띠데모한다/"
IF 던전플래그:31:9 == 0
	VALUES += "[2]쁘띠가 아닌 데모한다/"
ELSE
	VALUES += "[2]데모나이즈 한다/"
ENDIF
IF 던전플래그:31:9 == 1 && BASE:MASTER:LV >= 60
	VALUES += "[3]강화 데모나이즈/"
ELSEIF 던전플래그:31:9 == 2
	VALUES += "[3]ADD/"
ELSE
	VALUES += "/"
ENDIF
VALUES += "[4]스테이터스를 본다//"
VALUES += "[0]밖에 나간다"

CALL INPUT_SELECT_D, VALUES, , , 2
IF RESULT == 4
	CALL SHOP_COM_301
ELSEIF RESULT == 1
	$P_31_1
	CALL MESSAGE_WINDOW_D , "남자 점주" , "자 그럼, 오늘은 누구를 쁘띠데모하는거지?"
	CALLFORM PETIT_DEMON_TARGET_COUNT, 1
	IF RESULT == 0
		CALL MESSAGE_WINDOW_D , "남자 점주" , "아라, 쁘띠데모받을 아이가 없네/말할것도 없지만, 그럼 쁘띠데모는 안되용"
		GOTO P_31
	ENDIF
	CALLFORM PETIT_DEMON_TARGET, 1
	IF RESULT == 0
		GOTO P_31
	ELSE
		CHARA = POS(RESULT)
	ENDIF
	CALL MESSAGE_WINDOW_D, "남자 점주", @"%CALLNAME:CHARA% 쨩한테 쁘띠데모 할꺼야?"
	CALL INPUT_YN_D , "쁘띠데모 시킨다" , "그만둔다"
	SIF RESULT == 1
		GOTO P_31_1
	$P_31_2
	CALL MESSAGE_WINDOW_D, "남자 점주", "자 그럼, 어떤 쁘띠데모 할꺼야?"
	CALLFORM PETIT_DEMON_SELECT, 1
	IF RESULT == 0
		GOTO P_31
	ELSE
		LOCAL = 0
		LOCAL = RESULT
	ENDIF
	CALLFORM PETIT_DEMON_TYPE_{LOCAL}, "체크", CHARA
	IF RESULT == 0
		GOTO P_31_2
	ENDIF
	CALLFORM PETIT_DEMON_TYPE_{LOCAL}, "확인", CHARA
	CALLFORM PETIT_DEMON_TYPE_{LOCAL}, "가격", CHARA
	IF MONEY:1 < RESULT
		CALL MESSAGE_WINDOW_D , "남자 점주" , "…어머, 당신/마카가 부족하네/다른걸로 하세요"
		GOTO P_31_2
	ENDIF
	CALL INPUT_YN_D , "하도록 한다" , "그만둔다"
	IF RESULT == 1
		GOTO P_31_2
	ENDIF
	CALLFORM PETIT_DEMON_TYPE_{LOCAL}, "가격", CHARA
	MONEY:1 -= RESULT
	;プチデモ実行
	CALLFORM PETIT_DEMON_SET_{LOCAL}, CHARA
	GOTO P_31_2
ELSEIF RESULT == 2
	CALL MESSAGE_WINDOW_D , "남자 점주" , "쁘띠가 아닌 데모나이즈, 구나?"
	IF 던전플래그:31:9 == 0
		CALL MESSAGE_WINDOW_D , "" , "＞그렇게 말하면, 점주의 눈이 날카로워졌다"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "…그렇네, 일단 본래의 데모나이즈에 대해서 설명할까요"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "우리집에서 평소 하는 쁘띠데모는/악마의 「모습」만을 인간의 신체에 도입해요/그래서 쁘띠데모한 뒤에도 그 사람은 어엿한 인간이야"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "하지만, 진짜 데모나이즈는 틀려요/원래 데모나이즈는, 인간을 통째로 악마로 만들기 위한 기술이라/데빌 소스를 듬뿍 섞어, 요"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "통상의 악마 합체로도 충분하지 않나, 라고 생각할 수도 있겠지만/악마한테 의식을 빼앗기지 않는다는 건 큰 이익이에요/악마 자체는 사용하지 않으니까, 당연하지만"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "물론 문제도 있어요/인간이 사용하기 위한 장비는 못쓰고, 내성도 악마의 것처럼 되요/넣는 데빌 소스로 어느 정도 조절은 되지만요"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "그리고 인간을 위한 특수기능은 거의 못쓰게 돼요. 예를 들면 악마변신능력자의 경우 변신능력은 상실되는 경우라던가."
		CALL MESSAGE_WINDOW_D , "남자 점주" , "그리고 물론, 데모나이즈된 누군가는 인간이 아니게 돼요/그 아이의 인간의 영혼은 끝나고, 몇번을 환생해도 악마 그대로"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "그래도, 누군가를 데모나이즈할래요?"
		던전플래그:31:9 = 1
	ELSE
		CALL MESSAGE_WINDOW_D , "남자 점주" , "알고 있겠지만/정말 데모나이즈한다면, 그 아이는 인간 아니게되요"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "그래도 괜찮은거죠?"
	ENDIF
	CALL INPUT_YN_D , "상관없다" , "역시 그만둔다"
	IF RESULT == 1
		GOTO P_31
	ENDIF
	$P_31_3
	CALL MESSAGE_WINDOW_D , "남자 점주" , "그렇네/그러면 누구를 데모나이즈할래요?"
	CALLFORM PETIT_DEMON_TARGET_COUNT, 2
	LOCAL = RESULT
	IF LOCAL == 0
		CALL MESSAGE_WINDOW_D , "남자 점주" , "어머, 데모나이즈해줄 수 있는 아이가 없네/ 말할 것도 없지만, 그러면 데모나이즈는 못해용"
		GOTO P_31
	ENDIF
	CALLFORM PETIT_DEMON_TARGET, 2
	IF RESULT == 0
		GOTO P_31
	ELSE
		CHARA = POS(RESULT)
	ENDIF
	CALL MESSAGE_WINDOW_D, "남자 점주", @"%CALLNAME:CHARA% 쨩을 데모나이즈할꺼에요?"
	CALL INPUT_YN_D , "데모나이즈시킨다" , "그만둔다"
	SIF RESULT == 1
		GOTO P_31_3
	$P_31_4
	
	CALL MESSAGE_WINDOW_D , "남자 점주" , "그래, 정말 하는거네/넣을 데빌 소스를 골라요/여러가지 있으니까, 느긋하게 골라요?"
	BF_FLAG = 0
	;なお, この항목の台詞たちは原作と구상と妄想を元に構築されています
	;なので, 原作と異なる입調の可能性があります
	CALLFORM PETIT_DEMON_SELECT, 2
	IF RESULT == 0
		GOTO P_31_3
	ELSE
		LOCAL = 0
		LOCAL = RESULT
		SIF LOCAL == 3114 && TALENT:CHARA:유아체형 == 1
			BF_FLAG = 99
	ENDIF
	
	CALLFORM DEMONIEZD_TYPE_{LOCAL}, "해설", CHARA
	CALLFORM DEMONIEZD_TYPE_{LOCAL}, "확인", CHARA
	CALLFORM DEMONIEZD_TYPE_{LOCAL}, "가격", CHARA
	IF MONEY:1 < RESULT
		CALL MESSAGE_WINDOW_D , "남자 점주" , "…뭐랄까, 마카가 부족하네/이러면 데모나이즈드해 줄수 없어용"
		GOTO P_31
	ENDIF
	CALL INPUT_YN_D , "예" , "아니오"
	SIF RESULT == 1
		GOTO P_31_4
	CALLFORM DEMONIEZD_TYPE_{LOCAL}, "가격", CHARA
	MONEY:1 -= RESULT
	CALLFORM DEMONIEZD_TYPE_{LOCAL}, "デモナイズ実行", CHARA
	;共通処理
	;이능자이외の전투資質は삭제
	;(도구지식については一応残す)
	TALENT:CHARA:석세서 = 0
	TALENT:CHARA:건슬링거 = 0
	;デモナイズ대상が페르소나구사자なら
	IF TALENT:CHARA:페르소나구사자
		;페르소나共通枠からデモナイズ대상の持ってる페르소나を探してくる
		FOR サーチ変数, 1, 11
			SIF DITEMTYPE:(サーチ変数):0 == 0
				CONTINUE
			LOCAL:1 = FINDCHARA_ID(DITEMTYPE:サーチ変数:페르소나("페르소나소지자"))
			SIF CHARA == LOCAL:1
				CALL PERSONA_UNEQUIPMENT(LOCAL:1, サーチ変数)
		NEXT
		;페르소나剥ぎ終わったら스킬초기化
		ABL:CHARA:스킬1 = 0
		ABL:CHARA:스킬2 = 0
		ABL:CHARA:스킬3 = 0
		ABL:CHARA:스킬4 = 0
		ABL:CHARA:스킬5 = 0
		ABL:CHARA:스킬6 = 0
		ABL:CHARA:스킬7 = 0
		ABL:CHARA:스킬8 = 0
		ABL:CHARA:스킬9 = 0
	ENDIF
	TALENT:CHARA:페르소나구사자 = 0
	TALENT:CHARA:데빌시프터 = 0
	TALENT:CHARA:식노 = 0
	TALENT:CHARA:악마빙의자 = 0
	;당신이외は서머너も삭제
	SIF NO:MASTER != NO:CHARA
		TALENT:CHARA:서머너 = 0
	;技能も삭제
	TALENT:CHARA:도술刀術 = 0
	TALENT:CHARA:팬서 = 0
	TALENT:CHARA:궁술 = 0
	TALENT:CHARA:검술 = 0
	TALENT:CHARA:창술 = 0
	TALENT:CHARA:도끼술 = 0
	TALENT:CHARA:채찍술 = 0
	TALENT:CHARA:투척술 = 0
	TALENT:CHARA:제사 = 0
	TALENT:CHARA:봉술 = 0
	TALENT:CHARA:마샬아츠 = 0
	;ついでに이능자なかったら이능자持たせる(元인간なところを손数の多さ(스킬枠)で表現)
	SIF TALENT:CHARA:이능자 == 0 && TALENT:CHARA:달인 == 0
		TALENT:CHARA:이능자 = 1
	;장비を全해제
	CALL 모든장비해제(CHARA)
	;생득무기のみ残る可能性があるのでここで念入りに殺す
	EQUIP:CHARA:검 = 0
	
	;実行時구상
	IF LOCAL == 3114
		CALLFORM DEMONIEZD_SET_{LOCAL}, CHARA, BF_FLAG
	ELSE
		CALLFORM DEMONIEZD_SET_{LOCAL}, CHARA
	ENDIF
	
	CALL MESSAGE_WINDOW_D, "남자 점주", "새로운 몸, 마음에 들었을까?"
	GOTO P_31
ELSEIF RESULT == 3
	;ここから捏造。デモナイズドしたキャラを마카払って내성を강화する
	CALL MESSAGE_WINDOW_D , "남자 점주" , "강화데모나이즈, 구나?"
	IF 던전플래그:31:9 == 1
		CALL MESSAGE_WINDOW_D , "" , "＞그렇게 말하면, 점주의 눈이 날카로워졌다"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "…………말해 두지만, 이건 본래는 금지에요"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "한때 여러 다른 악마에서 데모나이즈드를 겹쳐서/보다 힘센 악마를 인공적으로 만들자는 프로젝트가 있었어요…"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "악어 자체를 사용하지 않아서 폭주할 위험은 비교적 적지만/확실성도 없고 비용도 만만치 않아요/내친김에 말하자면 견딜 실험체 확보도 안됐고…"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "결국 그 프로젝트는 실패해서 무기한 동결되었어요…/하지만, 그 연구 성과의 일부는…뭐 얘긴 여기까지만 할까요"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "뭐 간단하게 말하자면 데빌소스를 어찌저찌해서 /데모나이즈드한 악마를 강화시켜버리자는 이야기에요!"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "어드밴스·데모나이즈/줄여서 「ADD」/그래서, 그래서 누구를 ADD하는거죠?"
		던전플래그:31:9 = 2
	ELSE
		CALL MESSAGE_WINDOW_D , "남자 점주" , "알고 있다고 생각하지만/이건 뒤쪽 업계에서도 극비니까?"
		CALL MESSAGE_WINDOW_D , "남자 점주" , "그래서, ADD할 거에요?"
	ENDIF
	CALL INPUT_YN_D , "예" , "아니오"
	IF RESULT == 1
		GOTO P_31
	ENDIF
	$P_31_5
	CALL MESSAGE_WINDOW_D , "남자 점주" , "그렇네 / 그럼 누구를 ADD 시켜줄꺼에요?"
	CALLFORM PETIT_DEMON_TARGET_COUNT, 3
	LOCAL = RESULT
	IF LOCAL == 0
		CALL MESSAGE_WINDOW_D , "남자 점주" , "어머, ADD해줄 수 있는 아이가 없네/ 말할 것도 없지만, 그러면 ADD는 못해용"
		GOTO P_31
	ENDIF
	CALLFORM PETIT_DEMON_TARGET, 3
	IF RESULT == 0
		GOTO P_31
	ELSE
		CHARA = POS(RESULT)
	ENDIF
	CALL MESSAGE_WINDOW_D, "남자 점주", @"%CALLNAME:CHARA% 쨩을 ADD할꺼에요?"
	CALL INPUT_YN_D , "ADD 한다" , "그만둔다"
	SIF RESULT == 1
		GOTO P_31_5
	CALL MESSAGE_WINDOW_D , "남자 점주" , "그래, 정말 하는거네"
	$P_31_6
	CALL MESSAGE_WINDOW_D , "남자 점주" , "그럼 들어갈 데빌소스를 골라주세요/덧붙여서 이 데빌소스는 안전을 보장할 수 없어요"
	CALLFORM PETIT_DEMON_SELECT, 3
	IF RESULT == 0
		GOTO P_31_5
	ELSE
		LOCAL = 0
		LOCAL = RESULT
	ENDIF
	CALLFORM ADVANCE_DEMONIEZD_TYPE_{LOCAL}, "해설", CHARA
	CALLFORM ADVANCE_DEMONIEZD_TYPE_{LOCAL}, "체크", CHARA
	IF RESULT == 0
		GOTO P_31_6
	ENDIF
	;스트레스が溜まってたら除外する
	IF CFLAG:CHARA:스트레스치 > 60
		CALL MESSAGE_WINDOW_D , "남자 점주" , @"…에, %CALLNAME:CHARA% 짱이 한계인가 보네/오늘은 그만둬요."
		GOTO P_31_5
	ENDIF
	CALLFORM ADVANCE_DEMONIEZD_TYPE_{LOCAL}, "확인", CHARA
	CALLFORM ADVANCE_DEMONIEZD_TYPE_{LOCAL}, "가격", CHARA
	IF MONEY:1 < RESULT
		CALL MESSAGE_WINDOW_D , "남자 점주" , "…뭐랄까, 마카가 부족하네/이러면 ADD해 줄수 없어용"
		GOTO P_31
	ENDIF
	CALL INPUT_YN_D , "예" , "아니오"
	SIF RESULT == 1
		GOTO P_31_5
	CALLFORM ADVANCE_DEMONIEZD_TYPE_{LOCAL}, "가격", CHARA
	MONEY:1 -= RESULT
	CALLFORM ADVANCE_DEMONIEZD_TYPE_{LOCAL}, "アドバンスド実行", CHARA

	;共通処理
	;一日でそう何度も강화できないようにするため, 스트레스치を加算
	CFLAG:CHARA:스트레스치 += 30

	;구상…は面倒なので全部一括
	IF NO:MASTER == NO:CHARA
		CALL MESSAGE_WINDOW_D, "남자 점주", "그럼 이쪽으로 와줄 수 있으려나?"
		CALL MESSAGE_WINDOW_D, "", @"＞가게 주인의 안내로 옷을 벗은 %마스터는% 작은 캡슐에 담겨졌다…"
