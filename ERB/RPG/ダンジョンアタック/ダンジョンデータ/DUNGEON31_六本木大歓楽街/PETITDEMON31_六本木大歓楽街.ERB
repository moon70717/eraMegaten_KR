	IF ARG == 2
		;デモナイズド
		IF POS(LOCAL) > -1 && ABL:POS(LOCAL):종족 == 0  && (TALENT:POS(LOCAL):3 || TALENT:POS(LOCAL):4 || TALENT:POS(LOCAL):5 || TALENT:POS(LOCAL):6 || TALENT:POS(LOCAL):7 || TALENT:POS(LOCAL):8 || NO:POS(LOCAL) == NO:MASTER)
			TARGET_COUNT += 1
		ENDIF
	ELSEIF ARG == 3
		;강화デモナイズド(악마인限定)
		IF POS(LOCAL) > -1 && ABL:POS(LOCAL):종족 == 73  && (TALENT:POS(LOCAL):3 || TALENT:POS(LOCAL):4 || TALENT:POS(LOCAL):5 || TALENT:POS(LOCAL):6 || TALENT:POS(LOCAL):7 || TALENT:POS(LOCAL):8 || NO:POS(LOCAL) == NO:MASTER)
			TARGET_COUNT += 1
		ENDIF
	ELSE
		;プチデモ
		IF POS(LOCAL) > -1 && ABL:POS(LOCAL):종족 == 0 && LOCAL:2 != 2
			TARGET_COUNT += 1
		ENDIF
	ENDIF
NEXT
RETURN TARGET_COUNT

