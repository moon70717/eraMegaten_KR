;傷アリの女子中学生です
;身請けはできず援助交際のみです
@SORP_PLAY_1021, ARGS
#DIM CHARA, 1
#LOCALSSIZE 1
SELECTCASE ARGS
	CASE "娼婦番号"
		RESULTS = [1021]
	CASE "キャラ名"
		RESULTS = 상처 있는 여중생
	CASE "指名名"
		RESULTS = 상처 있는 여중생
	CASE "所属"
		;表なら1、裏なら2、援助なら3
		RETURN 3
	CASE "判定"
		;指名可能なら0、不可なら1
		IF 롯폰기창관플래그:21 >= 3
			RETURN 1
		ELSEIF 투기장플래그:10:0 == 1 && 투기장플래그:10:1 == 0
			RETURN 0
		ELSE
			RETURN 1
		ENDIF
	CASE "価格"
		RETURN 50
	CASE "紹介文"
		IF 롯폰기창관플래그:21 == 0
			CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "그녀는―/구강 섹스로 원조해주었으면 하는 것 같습니다―"
			CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "얼굴에 상처가 있어서―/기피하는 사람도 많지만―/결국 섹스 하난 잘 하거든요―"
			CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "왜인지 이유 있는 것 같아서―/어디선가 도망쳐 왔다든가―/돈이 많이 있다던가―"
		ELSE
			CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "그 애의 입―/버릇이 되어버리셨나요―?"
			CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "망가진 COMP를 고쳐서―/동료마를 다시 모으기 위해―/아직 돈이 있다던가―"
		ENDIF
	CASE "確認"
		CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "50 마카로 원조를 희망하고 있는데―/어떠십니까―?"
	CASE "プレイ口上"
		IF 롯폰기창관플래그:21 == 0
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "…………"
			CALL MESSAGE_WINDOW_D, "", "＞나타난 것은 얼굴에 상처가 난 13∼15세 정도의 무뚝뚝한 여자애다/＞상처가 있지만 예쁜 얼굴에 알맞게 발육한 몸을 기대할 수 있다/＞……어디선가 본 적이 있는 것 같았다"
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "…………어?/……어째서, 여기에"
			CALL MESSAGE_WINDOW_D, "", "＞어디서 만났나? 소녀에게 물어봤지만, 소녀는 불쾌한 듯이 고개를 내저었다"
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……아무것도/……빨리, 하자"
			CALL MESSAGE_WINDOW_D, "", "………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
					IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
						CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……으음, 츄븝, 우음……츄릅……/……읏, 으응, 크……쥬릇……/케흡, 으에……엑"
						CALL MESSAGE_WINDOW_D, "", @"＞상처 있는 여중생은 %마스터가% 입안에 뱉은 정액을 토해냈다/＞그 표정은 괴로워 보여서 익숙지 않은 것 같았다…"
						CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "…………만족했어?/……그럼, 빨리, 마카 줄래/으에, 아직도 입에 남았어……"
					ELSE
						CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……으음, 핥짝, 우음……츄븝……/……윽, 으응, 크……으……읏/므으, 그……윽"
						CALL MESSAGE_WINDOW_D, "", @"＞상처 있는 여중생은 %CALLNAME:MASTER%의 애액으로 더러워진 입술을 소매로 닦았다/＞그 표정은 괴로워 보여서 익숙지 않은 것 같았다…"
						CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "…………만족했어?/……그럼, 빨리, 마카 줄래/왜, 이런 데 핥게 하는 걸까……"
					ENDIF
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……마카만 잘 준다면, 또 와도 돼/어지간해선 매일 밤 여기에 있으니까……"
			;経験フラグを設定
			롯폰기창관플래그:21 = 1
		ELSE
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……또 온 거야?/……뭐 좋지만/마카만, 제대로 준다면"
			CALL MESSAGE_WINDOW_D, "", "………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
					IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
						CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……음, 츄븝, 우음……츄릅……/……읏, 으응, 크……쮸웁……/푸하……아"
						CALL MESSAGE_WINDOW_D, "", @"＞상처 있는 여중생은 %마스터가% 입안에 뱉은 정액을 삼켰다/＞태도는 딱딱하지만, 빨아들이는 듯한 정중한 봉사였다…"
						CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "…………만족했어?/……그럼, 빨리, 마카 줄래/왜, 이런 거 삼키게 하는 걸까……"
					ELSE
						CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……으음, 핥짝, 우음……츄븝……/……윽, 으응, 크……으……읏/푸하……아"
						CALL MESSAGE_WINDOW_D, "", @"＞상처 있는 여중생은 %CALLNAME:MASTER%의 애액으로 더러워진 입술을 혀로 핥았다/＞태도는 딱딱하지만, 숙달된 정중한 봉사였다…"
						CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "…………만족했어?/……그럼, 빨리, 마카 줄래/기분 좋았다면, 다행이고……"
					ENDIF
			;見受け可能フラグを設定
			롯폰기창관플래그:21 = 2
		ENDIF
		;プレイ経験(任意の経験を追加可)
		EXP:MASTER:절정경험 += ABL:MASTER:기교
		JUEL:MASTER:쾌Ｃ += ABL:MASTER:기교 * (50) + 50
		JUEL:MASTER:욕정 += ABL:MASTER:기교 * (50) + 50
		IF !TALENT:MASTER:남자
			EXP:MASTER:레즈경험 += ABL:MASTER:기교 * 1 + 1
		ENDIF
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			EXP:MASTER:사정경험 += ABL:MASTER:기교
			BASE:MASTER:ＭＡＧ -= ABL:MASTER:기교 * 500
		ENDIF
		CALL INCREASE_LC,5
	CASE "身受け価格"
		RETURN 200
	CASE "身受け確認"
		CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "저 애의 보지, 흥미있나요―?/돈이 더 필요할 거 같아서―/얼마 전부터 본방 시작했거든요―"
		CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "고정 손님도 몇 명 있는 것 같아서―/조임이 좋다고 소문이 자자하다구요―?"
		CALL MESSAGE_WINDOW_D, "노출한 웨이트리스", "200 마카를 내면 본방에 들어가도 좋다고 하는데―/어떠신가요―?"
	CASE "身受け口上"
	;キャラ加入はしません。遊べるだけです。
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……그래, 당신이구나/……딱히/……누구라도 다른 건 없어"
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……섹스, 할 거잖아?/……빨리, 하러 가자"
			CALL MESSAGE_WINDOW_D, "", "………………/…………/……"
			CALL SHOW_PICTURE, "EMPTY"
			SELECTCASE RAND:3
				CASE 0
					;路地裏エッチ
					CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "으아……앗, 안돼, 안됏……, 이런, 곳에서……!?"
					CALL MESSAGE_WINDOW_D, "", @"＞%마스터는% 호텔 가는 시간도 아까워서 상처 있는 여중생을 골목에서 범했다/＞말과는 달리, 그 질은 상태 좋게 육봉을 물고 있었다…"
					CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "읏!?　싫엇, 보여지고……있으, 니까……앗……!?"
					CALL MESSAGE_WINDOW_D, "", @"＞히죽거리며 구경하는 관중들에게 보여주듯 %마스터는% 상처 있는 여중생의 질내를 정액으로 더럽혔다…"
				CASE 1
					;ラブホエッチ
					CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "아……, 큿……, 하아……앗……응흣……"
					CALL MESSAGE_WINDOW_D, "", @"＞%마스터는% 러브호텔의 어느 방에 상처 있는 여중생을 데려오자마자 그 자리에서 밀어 넘어뜨렸다/＞소녀의 질내는 나이에 걸맞지 않게 잘 익은 상태로 육봉을 받아들였다…"
					CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "……이제, 빼줘……, 마지막은, 입으로……읏……"
					CALL MESSAGE_WINDOW_D, "", @"＞그 말에 따라 %마스터는% 상처 있는 여중생의 질내에서 육봉을 빼고, 소녀의 얼굴을 가까이 당겨 입안에 사정했다…"
				CASE 2
					;トイレエッチ
					CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "꺅……, 싫어, 싫어엇……, 이런, 불결한……읏!?"
					CALL MESSAGE_WINDOW_D, "", @"＞%마스터는% 호텔 가는 시간도 아까워서 상처 있는 여중생을 남자 화장실에서 범했다/＞말과는 달리, 그 질은 상태 좋게 육봉을 물고 있었다…"
					CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "읏!?　아, 끄으……안은, 안돼……엣……!?"
					CALL MESSAGE_WINDOW_D, "", @"＞그 말을 무시하고 %마스터는% 상처 있는 여중생의 질내를 정액으로 더럽혔다…"
				ENDSELECT
		ELSE
			;レズエッチ
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "이, 이걸로, 좋아……?　기분 좋아……?"
			CALL MESSAGE_WINDOW_D, "", @"＞%마스터는% 러브호텔의 어느 방에서 상처 있는 여중생에게 페니스 밴드를 장착시키고 허리를 흔들게 했다/＞당황한 듯한 말과는 달리 어딘가 흥분하고 있는 것 같다…"
			CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "읏, 이, 이제 가버리는거야……?　이런 거로, 가는거야……아?"
			CALL MESSAGE_WINDOW_D, "", @"＞풋풋한 상처 있는 여중생의 허리놀림이 귀엽게 느껴져서 %마스터는% 살짝 절정했다…"
		ENDIF
		CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "…………/……별로, 화난 건 아니야/……됐으니까, 빨리 마카나 줘"
		CALL MESSAGE_WINDOW_D, "상처 있는 여중생", "응, 마카, 고마워……/……또 마카 준다면, 또 와도 좋아/……그래도, 너무 이상한 건, 싫으니까"
		EXP:MASTER:성교경험 += ABL:MASTER:기교 * ABL:MASTER:욕망 * 2 + 1
		EXP:MASTER:절정경험 += ABL:MASTER:기교 * ABL:MASTER:욕망
		JUEL:MASTER:쾌Ｃ += ABL:MASTER:기교 * ABL:MASTER:욕망 * 300 + 500
		JUEL:MASTER:욕정 += ABL:MASTER:기교 * ABL:MASTER:욕망 * 200 + 500
		JUEL:MASTER:습득 += ABL:MASTER:기교 * ABL:MASTER:욕망 * 200 + 500
		IF !TALENT:MASTER:남자
			EXP:MASTER:레즈경험 += ABL:MASTER:기교 * ABL:MASTER:욕망 * 2 + 1
			JUEL:MASTER:쾌Ｖ +=  ABL:MASTER:기교 * ABL:MASTER:욕망 * 100 + 300
		ENDIF
		IF TALENT:MASTER:남자 || TALENT:MASTER:후타나리
			EXP:MASTER:질사경험 += ABL:MASTER:욕망
			EXP:MASTER:사정경험 += ABL:MASTER:욕망 + 1
			BASE:MASTER:ＭＡＧ -=  ABL:MASTER:기교 * ABL:MASTER:욕망 * 2000 + 1000
		ENDIF
	CASE "宣伝"
		;
ENDSELECT

